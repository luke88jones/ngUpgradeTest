{
  "name": "ngupgradetest",
  "version": "1.0.0",
  "description": "NgUpgrade Test",
  "directories": {
    "test": "test"
  },
  "typings": "./typings/component.d.ts",
  "scripts": {
    "build": "npm run compile",
    "compile": "webpack && npm run lint",
    "clean": "rimraf dist && rimraf tmp",
    "develop": "npm run sass && concurrent --kill-others \"webpack --watch\" \"node-sass --watch --output ./dist/css ./src/sass\" \"live-server --port=3000 --quiet --open=src/\"",
    "lint": "node ./scripts/ts-lint.js -f src/ts/**/*.ts",
    "postinstall": "node ./scripts/update-sass.js && bower install && typings install",
    "test": "karma start karma.unit.js --single-run",
    "test-watch": "karma start karma.unit.js",
    "test-karma": "karma start karma.coverage.js",
    "watch": "rimraf tmp && webpack --watch",
    "watch-karma": "rimraf tmp && npm run compile && concurrent --kill-others \"webpack --watch\" \"karma start karma.unit.js --singleRun=false --autoWatch=true\"",
    "ci-tc": "webpack && karma --env=tc start karma.coverage.js",
    "ci-jk": "webpack && karma --env=jk start karma.coverage.js",
    "copy-sass": "node ./scripts/copy.js ./src/**/_*.scss ./dist/sass",
    "dist": "npm run clean && npm run mkdir && npm run build && npm run test && npm run sass && npm run copy-sass && npm run uglify && npm run process-html",
    "mkdir": "node ./node_modules/mkdirp/bin/cmd ./tmp ./dist/js ./dist/css ./dist/sass",
    "sass": "node-sass --output ./dist/css ./src/sass/",
    "process-html": "htmlprocessor src/index.html -o dist/index.html",
    "serve": "live-server --open=dist/",
    "uglify": "webpack --minimize"
  },
   "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",

    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23",
    "angular2-in-memory-web-api": "0.0.20",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
    "clean-css": "^3.4.18",
    "concurrently": "^2.2.0",
    "cp": "^0.2.0",
    "glob": "^7.0.3",
    "globby": "^5.0.0",
    "htmlprocessor": "^0.2.3",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "1.0.0",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.0.0",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-teamcity-reporter": "^1.0.0",
    "karma-typescript-preprocessor": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "live-server": "1.1.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "ts-loader": "^0.8.2",
    "tslint": "^3.11.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "webpack": "^1.13.1"
  }
}